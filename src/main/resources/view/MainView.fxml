<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minHeight="500.0" minWidth="500.0"
	spacing="10.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.MainView">
	<children>
		<fx:include source="menu/MenuView.fxml" />
		<fx:include source="gameoptions/GameOptionsView.fxml" />
		<fx:include source="grids/GridView.fxml" />
		<HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
			<children>
				<Label text="row:" />
				<Label fx:id="labelRow" />
				<Label text="column:" />
				<Label fx:id="labelColumn" />
				<Label text="id:" />
				<Label fx:id="labelID" />
			</children>
		</HBox>
		<!-- <fx:include source="menu/ProgressView.fxml" /> -->
	</children>
</VBox>
